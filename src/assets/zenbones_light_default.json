    "tokenColors": [
        {
            "scope": [
                "comment"
            ],
            "name": "Comment",
            "settings": {
                "foreground": "#948985"
            }
        },
        {
            "scope": [
                "variable.other.constant",
                "constant",
                "string"
            ],
            "name": "Constant",
            "settings": {
                "foreground": "#556570"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "name": "Number",
            "settings": {
                "foreground": "#2C363C"
            }
        },
        {
            "scope": [
                "constant.language.boolean"
            ],
            "name": "Boolean",
            "settings": {
                "foreground": "#2C363C"
            }
        },
        {
            "scope": [
                "variable.other"
            ],
            "name": "Identifier",
            "settings": {
                "foreground": "#44525B",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.function"
            ],
            "name": "Function",
            "settings": {
                "foreground": "#2C363C",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "keyword"
            ],
            "name": "Statement",
            "settings": {
                "foreground": "#2C363C",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "keyword.control.directive"
            ],
            "name": "PreProc",
            "settings": {
                "foreground": "#2C363C",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "storage.type",
                "storage.modifier",
                "keyword.other.typedef"
            ],
            "name": "Type",
            "settings": {
                "foreground": "#6A5549",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "punctuation.definition.template-expression"
            ],
            "name": "Special",
            "settings": {
                "foreground": "#4F5E68",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "entity.name.tag"
            ],
            "name": "Tag",
            "settings": {
                "foreground": "#4F5E68",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "support.type.property-name"
            ],
            "name": "PropertyName",
            "settings": {
                "foreground": "#2C363C"
            }
        }
